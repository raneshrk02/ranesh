    "import random\n",
    "a = str(input(\"Choose heads or tails: \")).lower()\n",
    "if a == \"heads\":\n",
    "  b = 1\n",
    "elif a == \"tails\":\n",
    "  b = 2\n",
    "\n",
    "c = random.randint(1,2)\n",
    "\n",
    "if c == 1:\n",
    "  print(\"Coin shows heads\")\n",
    "elif c == 2:\n",
    "  print(\"Coin shows tails\")\n",
    "\n",
    "if b == c:\n",
    "  print(\"You win\")\n",
    "elif b != c:\n",
    "  print(\"You lose\")"
   ]
